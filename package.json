{
  "name": "tscomp",
  "version": "0.15.0",
  "description": "Buildtool for Typescript projects",
  "license": "MIT",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": "Pajn/tscomp",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "tscomp": "tasks/tscomp.sh",
    "e2e": "tasks/e2e-simple.sh",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "format": "prettier --trailing-comma es5 --single-quote --write '!(node_modules)/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "files": [
    "babelrc",
    "eslintrc",
    "bin",
    "config",
    "scripts",
    "templates",
    "utils"
  ],
  "keywords": [
    "create-react-app",
    "typescript",
    "webpack",
    "build",
    "jest",
    "test",
    "generator"
  ],
  "bin": {
    "tscomp": "./bin/tscomp.js"
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "commander": "^2.9.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fork-ts-checker-notifier-webpack-plugin": "0.2.0",
    "fork-ts-checker-webpack-plugin": "0.2.8",
    "fs-extra": "3.0.1",
    "gulp": "4.0.0",
    "gulp-babel": "6.1.2",
    "gulp-changed-in-place": "2.3.0",
    "gulp-clone": "1.1.1",
    "gulp-plumber": "1.1.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "3.2.4",
    "html-webpack-plugin": "2.29.0",
    "hyperquest": "^2.1.2",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "merge2": "1.2.0",
    "nodemon": "1.12.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react-dev-utils": "4.1.0",
    "semver": "^5.3.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "tar-pack": "^3.4.0",
    "tmp": "0.0.31",
    "ts-loader": "3.4.0",
    "typescript": "~2.7.1",
    "typescript-babel-jest": "1.0.5",
    "url-loader": "0.6.2",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "@types/react": "^15.0.36",
    "@types/react-dom": "^15.5.1",
    "@types/webpack": "^3.0.7",
    "lint-staged": "^3.3.2",
    "prettier": "1.6.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "optionalDependencies": {
    "fsevents": "1.1.2"
  }
}
