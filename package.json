{
  "name": "tscomp",
  "version": "1.0.0-beta.2",
  "description": "Effortless buildtool for TypeScript projects",
  "license": "MIT",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": "Pajn/tscomp",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "tscomp": "tasks/tscomp.sh",
    "e2e": "tasks/e2e-simple.sh",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "format": "prettier --trailing-comma es5 --single-quote --write '!(node_modules)/**/*.js'"
  },
  "files": [
    "babelrc",
    "eslintrc",
    "bin",
    "config",
    "scripts",
    "templates",
    "utils"
  ],
  "keywords": [
    "create-react-app",
    "typescript",
    "webpack",
    "build",
    "jest",
    "test",
    "generator"
  ],
  "bin": {
    "tscomp": "./bin/tscomp.js"
  },
  "dependencies": {
    "@babel/core": "7.0.0-rc.1",
    "@babel/runtime": "7.0.0-rc.1",
    "@svgr/webpack": "^2.2.1",
    "autoprefixer": "9.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-named-asset-import": "1.0.0-next.3e165448",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react-app": "https://github.com/bappogroup/babel-preset-react-app#5067370a35f05b8aea05e22932157d256e35cd68",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "commander": "^2.17.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "1.1.11",
    "fork-ts-checker-notifier-webpack-plugin": "0.4.0",
    "fork-ts-checker-webpack-plugin": "0.4.9",
    "fs-extra": "7.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0-beta.2",
    "gulp-changed-in-place": "2.3.0",
    "gulp-clone": "2.0.1",
    "gulp-plumber": "1.2.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "5.0.0-alpha.3",
    "html-webpack-plugin": "^4.0.0-alpha",
    "hyperquest": "^2.1.2",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "merge2": "1.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "nodemon": "1.18.3",
    "object-assign": "4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.1",
    "react-dev-utils": "6.0.0-next.3e165448",
    "semver": "^5.5.1",
    "style-loader": "0.22.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "tar-pack": "^3.4.0",
    "terser-webpack-plugin": "^1.0.2",
    "thread-loader": "1.2.0",
    "tmp": "0.0.33",
    "ts-jest": "23.1.4",
    "ts-loader": "4.5.0",
    "typescript": "~3.0.1",
    "url-loader": "1.1.1",
