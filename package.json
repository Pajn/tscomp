{
  "name": "tscomp",
  "version": "0.12.0",
  "description": "Buildtool for Typescript projects",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "tscomp": "tasks/tscomp.sh",
    "e2e": "tasks/e2e-simple.sh",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "format": "prettier --trailing-comma es5 --single-quote --write '!(node_modules)/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "files": [
    "babelrc",
    "eslintrc",
    "bin",
    "config",
    "scripts",
    "templates",
    "utils"
  ],
  "keywords": [
    "create-react-app",
    "typescript",
    "webpack",
    "build",
    "jest",
    "test",
    "generator"
  ],
  "bin": {
    "tscomp": "./bin/tscomp.js"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "1.1.3",
    "commander": "^2.9.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.4",
    "detect-port": "^1.2.0",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "fork-ts-checker-notifier-webpack-plugin": "^0.2.0",
    "fork-ts-checker-webpack-plugin": "^0.2.6",
    "fs-extra": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed-in-place": "^2.2.0",
    "gulp-clone": "^1.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-typescript": "^3.2.0",
    "html-webpack-plugin": "^2.29.0",
    "http-proxy-middleware": "^0.17.4",
    "hyperquest": "^2.1.2",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "merge2": "^1.0.3",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.2",
    "react-error-overlay": "^1.0.9",
    "semver": "^5.3.0",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "0.9.1",
    "tar-pack": "^3.4.0",
    "tmp": "0.0.31",
    "ts-loader": "^2.2.2",
    "typescript": "^2.4.1",
    "typescript-babel-jest": "^1.0.4",
    "url-loader": "^0.5.9",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.5.1",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/react": "^15.0.36",
    "@types/react-dom": "^15.5.1",
    "@types/webpack": "^2.2.11",
    "lint-staged": "^3.3.2",
    "prettier": "^1.5.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "optionalDependencies": {
    "fsevents": "1.1.2"
  }
}
