{
  "name": "tscomp",
  "version": "1.0.0-beta.5",
  "description": "Effortless buildtool for TypeScript projects",
  "license": "MIT",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": "Pajn/tscomp",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "tscomp": "tasks/tscomp.sh",
    "e2e": "tasks/e2e-simple.sh",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "format": "prettier --trailing-comma es5 --single-quote --write '!(node_modules)/**/*.js'"
  },
  "files": [
    "babelrc",
    "eslintrc",
    "bin",
    "config",
    "scripts",
    "templates",
    "utils"
  ],
  "keywords": [
    "create-react-app",
    "typescript",
    "webpack",
    "build",
    "jest",
    "test",
    "generator"
  ],
  "bin": {
    "tscomp": "./bin/tscomp.js"
  },
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/runtime": "7.1.2",
    "@svgr/webpack": "2.4.1",
    "autoprefixer": "9.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-named-asset-import": "0.2.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-react-app": "Pajn/babel-preset-react-app",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "commander": "^2.17.1",
    "cross-spawn": "6.0.5",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "2.0.0",
    "fork-ts-checker-notifier-webpack-plugin": "0.6.2",
    "fork-ts-checker-webpack-plugin": "0.4.10",
    "fs-extra": "7.0.0",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0",
    "gulp-changed-in-place": "2.3.0",
    "gulp-clone": "2.0.1",
