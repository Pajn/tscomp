    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "prettier": "1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "optionalDependencies": {
    "fsevents": "1.2.4"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
