{
  "name": "tscomp",
  "version": "1.0.0-beta.0",
  "description": "Buildtool for Typescript projects",
  "license": "MIT",
  "author": "Rasmus Eneman <rasmus@eneman.eu>",
  "repository": "Pajn/tscomp",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "tscomp": "tasks/tscomp.sh",
    "e2e": "tasks/e2e-simple.sh",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "format": "prettier --trailing-comma es5 --single-quote --write '!(node_modules)/**/*.js'"
  },
  "files": [
    "babelrc",
    "eslintrc",
    "bin",
    "config",
    "scripts",
    "templates",
    "utils"
  ],
  "keywords": [
    "create-react-app",
    "typescript",
    "webpack",
    "build",
    "jest",
    "test",
    "generator"
  ],
  "bin": {
    "tscomp": "./bin/tscomp.js"
  },
