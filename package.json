    "@types/jest": "^23.3.5",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "prettier": "1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "optionalDependencies": {
    "fsevents": "1.2.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
