{
  "name": "tscomp",
  "version": "0.11.0",
  "description": "Buildtool for Typescript projects",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "tscomp": "tasks/tscomp.sh",
    "e2e": "tasks/e2e-simple.sh",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "format": "prettier --trailing-comma es5 --single-quote --write '!(node_modules)/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "files": [
    "babelrc",
    "eslintrc",
    "bin",
    "config",
    "scripts",
    "templates",
    "utils"
  ],
  "bin": {
    "tscomp": "./bin/tscomp.js"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
